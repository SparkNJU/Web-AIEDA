{
  "name": "aieda-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:5173": "vite --port 5173",
    "build": "vue-tsc -b && vite build",
    "build:analyze": "vue-tsc -b && vite build --mode analyze",
    "preview": "vite preview",
    "preview:80": "vite preview --port 80 --host 0.0.0.0",
    "serve": "npm run build && npm run preview:80",
    "serve:local": "vite preview --port 8080"
  },
  "dependencies": {
    "@types/katex": "^0.16.7",
    "axios": "^1.10.0",
    "element-plus": "^2.10.3",
    "katex": "^0.16.22",
    "markdown-it": "^14.1.0",
    "markdown-it-texmath": "^1.0.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.2.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vue-tsc": "^2.2.12"
  }
}
